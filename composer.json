    "description":"kumatch/keystore driver for AWS S3",
    "type":"library",
    "homepage": "https://github.com/kumatch/php-keystore-s3",
    "keywords":["storage", "file", "s3"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Yosuke Kumakura",
            "homepage": "https://github.com/kumatch"
        }
