        "aws/aws-sdk-php": "2.*",
        "kumatch/keystore": "*",
        "kumatch/keystore-filesystem": "*",
        "kumatch/path": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.*",
        "kumatch/fs-temp": "*"
